{
  "name": "robynm",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "sass": "node-sass ./stylesheets/style.scss ./stylesheets/style.css",
    "prefix": "postcss -u autoprefixer -r ./stylesheets/style.css",
    "css": "npm run sass && npm run prefix",
    "css:watch": "npm run sass && npm run prefix -- -w"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "node-sass": "^3.4.2",
    "postcss": "^5.2.4",
    "postcss-cli": "^2.6.0"
  }
}
